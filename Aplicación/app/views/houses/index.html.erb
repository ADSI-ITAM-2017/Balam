<p id="notice"><%= notice %></p>

<h1>Casas</h1>



<%= form_tag(houses_path, :method => "get", id: "search-form") do %>
  <%= text_field_tag :search, params[:search], placeholder: "Buscar" %>
  <%= submit_tag "Buscar" %>
<% end %>


<div class="col-md-8">
      <h3>CASAS (<%= @houses.count %>)</h3>
      <ol class="houses">
        <%  @houses = @houses.paginate(page: params[:page]) %>
        <%= render @houses %>
      </ol>
      <%= will_paginate @houses %>
</div>
  